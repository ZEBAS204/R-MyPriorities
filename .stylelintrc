{
  "extends": [
    "stylelint-scss",
    "stylelint-config-sass-guidelines",
    "stylelint-config-prettier"
  ],
  "rules": {
    "property-no-unknown": [true, { "severity": "warning" }],
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "selector-max-id": null,
    "max-nesting-depth": null
  }
}